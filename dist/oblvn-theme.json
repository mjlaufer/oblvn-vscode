  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment, punctuation.definition.comment",
      "settings": { "foreground": "#09BB07", "fontStyle": "italic" }
    },
    {
      "name": "Variable, Support",
      "scope": "variable, support.type, support.orther.namespace.use.php, meta.use.php, support.other.namespace.php, ",
      "settings": { "foreground": "#ABC7CE" }
    },
    {
      "name": "Keyword, Storage",
      "scope": "keyword, storage.type, storage.modifier",
      "settings": { "foreground": "#668B95" }
    },
    {
      "name": "Punctuation, Misc",
      "scope": "constant.other.color, punctuation, punctuation.definition.tag, punctuation.separator.inheritance.php, punctuation.definition.tag.html, punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html, punctuation.section.embedded, keyword.other.template, keyword.other.substitution",
      "settings": { "foreground": "#668B95" }
    },
    {
      "name": "Function, Special Method, Block Level",
      "scope": "entity.name.function, meta.function-call, variable.function, support.function, keyword.other.special-method, meta.block-level",
      "settings": { "foreground": "#4DA9BE" }
    },
    {
      "name": "Other Variable, String Link",
      "scope": "support.other.variable, string.other.link",
      "settings": { "foreground": "#DC444A" }
    },
    {
      "name": "Number, Constant, Function Parameter, Tag, Embedded",
      "scope": "constant.numeric, constant.language, support.constant, constant.character, variable.parameter, entity.name.tag, meta.tag.sgml, keyword.other.unit",
      "settings": { "foreground": "#FE6B49" }
    },
    {
      "name": "Symbols, Inherited Class, Markup Heading",
      "scope": "constant.other.symbol, constant.other.key, entity.other.inherited-class, markup.heading, meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
      "settings": { "foreground": "#09BB07" }
    },
    {
      "name": "Operator, Function Arrow, String, Class, Support",
      "scope": "keyword.operator, storage.type.function.arrow, string, entity.name.class, support.class, entity.name.type.class",
      "settings": { "foreground": "#BDC37F" }
    },
    {
      "name": "Sub-methods",
      "scope": "entity.name.module.js, variable.import.parameter.js, variable.other.class.js",
      "settings": { "foreground": "#DC444A" }
    },
    {
      "name": "Language methods",
      "scope": "variable.language",
      "settings": { "fontStyle": "italic", "foreground": "#DC444A" }
    },
    {
      "name": "entity.name.method.js, meta.method.js",
      "scope": "entity.name.method.js, meta.class-method.js entity.name.function.js, variable.function.constructor",
      "settings": { "foreground": "#ABC7CE" }
    },
    {
      "name": "Regular Expressions, Escape Characters",
      "scope": "string.regexp, constant.character.escape",
      "settings": { "foreground": "#668B95" }
    },
    {
      "name": "Attributes",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#668b95" }
    },
    {
      "name": "Inserted",
      "scope": "markup.inserted, markup.inserted.git_gutter",
      "settings": { "foreground": "#09BB07" }
    },
    {
      "name": "Changed",
      "scope": "markup.changed, markup.changed.git_gutter",
      "settings": { "foreground": "#BDC37F" }
    },
    {
      "name": "Deleted",
      "scope": "markup.deleted, markup.deleted.git_gutter",
      "settings": { "foreground": "#DC444A" }
    },
    { "name": "URL", "scope": "*url*, *link*, *uri*", "settings": { "fontStyle": "underline" } },
    {
      "name": "Search Results Nums",
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": { "foreground": "#FE6B49" }
    },
    {
      "name": "Search Results Lines",
      "scope": "entity.name.filename.find-in-files",
      "settings": { "foreground": "#09BB07" }
    },
    {
      "name": "Decorators",
      "scope": "tag.decorator.js entity.name.tag.js, tag.decorator.js punctuation.definition.tag.js",
      "settings": { "fontStyle": "italic", "foreground": "#4DA9BE" }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": "source.js constant.other.object.key.js string.unquoted.label.js",
      "settings": { "fontStyle": "italic", "foreground": "#DC444A" }
    },
    { "scope": "token.info-token", "settings": { "foreground": "#4DA9BE" } },
    { "scope": "token.warn-token", "settings": { "foreground": "#BDC37F" } },
    { "scope": "token.error-token", "settings": { "foreground": "#DC444A" } },
    { "scope": "token.debug-token", "settings": { "foreground": "#FE6B49" } },
    { "scope": "source.js entity.other.attribute-name.js", "settings": { "fontStyle": "italic" } },
    {
      "scope": "text.html.basic entity.other.attribute-name.html",
      "settings": { "fontStyle": "italic" }
    }
  ]
}
