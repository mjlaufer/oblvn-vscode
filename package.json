{
  "name": "oblvn-vscode",
  "displayName": "OBLVN",
  "description": "Minimalistic, high contrast, dark theme",
  "version": "0.1.0",
  "publisher": "mjlaufer",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/mjlaufer/oblvn-vscode"
  },
  "engines": {
    "vscode": "^1.14.0"
  },
  "keywords": [
    "dark",
    "dark theme",
    "high contrast",
    "oblivion",
    "oblvn"
  ],
